{
  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "webRequest-demo",
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "gaa@example.com",
      "strict_min_version": "42.0"
    }
  },

  "permissions": [
    "webRequest",
    "activeTab",
    "<all_urls>",
    "cookies",
    "nativeMessaging",
    "downloads",
    "webRequestBlocking",
    "tabs"
  ],

  "browser_action": {
    "default_icon" : "./icon.ico",
    "default_title" : "Gaana Downloader"
  },

  "background": {
    "scripts": ["background.js"]
  }
}